<%= form_with(model: @order, url: place_order_cart_path(@cart), method: :post) do |f| %>
  
  <div>
    <%= f.label :name, 'お名前' %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :phone_number, '電話番号' %>
    <%= f.text_field :phone_number %>
  </div>

  <div>
    <%= f.label :address, '住所' %>
    <%= f.text_field :address %>
  </div>

  <%= f.submit '注文を確定する' %>
<% end %>